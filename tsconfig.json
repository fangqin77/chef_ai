{
  "compilerOptions": {
    "target": "ES2018",
    "lib": ["ES2018", "DOM"],
    "moduleResolution": "Node",
    "module": "ESNext",
    "noEmit": false,
   "baseUrl": ".",
    "paths": {
      "@/*": ["./*"]
    },
    "checkJs": false
  },
  "vueCompilerOptions": {
    "globalTypesPath": "./types/vue-global.d.ts",
    "strictTemplates": false
  },
  "exclude": ["node_modules", "unpackage", "dist"]
}
